FoamFile
{
    format      ascii;
    class       surfaceVectorField;
    location    "32";
    object      U_avg_f;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    movingWall
    {
        type            calculated;
        value           uniform (0.01 0 0);
    }
    fixedWalls
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    frontAndBack
    {
        type            empty;
        value           nonuniform List<vector> 0();
    }
}


